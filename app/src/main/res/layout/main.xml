<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="190"
        android:layout_height="195dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/parking_icon"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="190"
        android:layout_height="195"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/Purchase"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="317dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="195"
        android:layout_height="195"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/balance"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="195"
        android:layout_height="195"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/help"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="317dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="379dp"
        android:layout_height="122dp"
        app:srcCompat="@drawable/siue_letters"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="192dp" />
</android.support.constraint.ConstraintLayout>